{
  "name": "kongzhi",
  "version": "0.0.0",
  "description": "A performant react controller",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/andykao1213/Kongzhi.git",
  "author": "Andrew Kao <andykao1213@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.2"
  },
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "dev": "yarn build --watch",
    "build": "rollup -c",
    "lint": "eslint --ignore-path .gitignore .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build test"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "rollup": "^2.45.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
